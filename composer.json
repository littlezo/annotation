{
  "name": "littler/think-annotation",
  "description": "Annotation For ThinkPHP6",
  "license": "MPL-2.0",
  "authors": [
    {
      "name": "@小小只^v^",
      "email": "littlezov@qq.com"
    }
  ],
  "require": {
    "topthink/framework": "^6.0",
    "doctrine/annotations": "^1.0",
    "php-di/phpdoc-reader": "^2.0",
    "symfony/class-loader": "^3.0",
    "erusev/parsedown": "^1.0"
  },
  "require-dev": {
    "topthink/think-ide-helper": "^v1.0"
  },
  "autoload": {
    "psr-4": {
      "littler\\annotation\\": "src"
    }
  },
  "extra": {
    "think": {
      "services": [
        "littler\\annotation\\Service"
      ],
      "config": {
        "annotation": "src/config.php"
      }
    }
  }
}
